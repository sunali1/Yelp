<p>
  <strong>Name</strong>
  <%= @restaurant.name %>
</p>
<p>
  <strong>Location</strong>
  <%= @restaurant.location %>
</p>
<p>
  <strong>Description</strong>
  <%= @restaurant.description %>
</p>
